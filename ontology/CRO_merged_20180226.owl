    <!-- http://purl.obolibrary.org/obo/CRO_0000811 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CRO_0000811">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CRO_0000823"/>
