    <!-- http://purl.obolibrary.org/obo/CRO_0002011 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CRO_0002011">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CRO_0002016"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/CRO_0000029"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/CRO_0000877"/>
